[
  {
    "id": "java-theia",
    "creator": "ide",
    "name": "Java Theia",
    "description": "Java + Theia in a sidecar container",
    "scope": "general",
    "tags": [
      "Java 1.8, Theia"
    ],
    "components": [
      {
        "name": "JDK",
        "version": "1.8.0_45"
      },
      {
        "name": "Maven",
        "version": "3.2.2"
      }
    ],
    "source": {
      "type": "image",
      "origin": "eclipse/ubuntu_jdk8"
    },
    "workspaceConfig": {
      "environments": {
        "default": {
          "machines": {
            "theia": {
              "attributes": {},
              "servers": {
                "theia": {
                  "port": "3000",
                  "attributes": {},
                  "protocol": "http"
                }
              },
              "volumes": {
                "projects": {
                  "path": "/projects"
                }
              },
              "installers": [
                "org.eclipse.che.terminal"
              ],
              "env": {}
            },
            "dev-machine": {
              "attributes": {},
              "servers": {
                "tomcat8-debug": {
                  "port": "8000",
                  "attributes": {},
                  "protocol": "http"
                },
                "codeserver": {
                  "port": "9876",
                  "attributes": {},
                  "protocol": "http"
                },
                "tomcat8": {
                  "port": "8080",
                  "attributes": {},
                  "protocol": "http"
                }
              },
              "volumes": {
                "projects": {
                  "path": "/projects"
                }
              },
              "installers": [
                "org.eclipse.che.exec",
                "org.eclipse.che.terminal"
              ],
              "env": {}
            }
          },
          "recipe": {
            "type": "compose",
            "content": "services:\n theia:\n  image: theiaide/theia\n  mem_limit: 1073741824\n dev-machine:\n  image: eclipse/ubuntu_jdk8\n  mem_limit: 2147483648\n  depends_on:\n    - theia",
            "contentType": "application/x-yaml"
          }
        }
      },
      "defaultEnv": "default",
      "name": "theia-workspace",
      "projects": [],
      "commands": [
        {
          "commandLine": "echo 'hello' && ls -la $HOME/'",
          "name": "test command",
          "attributes": {},
          "type": "custom"
        }
      ],
      "links": []
    },
    "stackIcon": {
      "name": "type-java.svg",
      "mediaType": "image/svg+xml"
    }
  }
]
